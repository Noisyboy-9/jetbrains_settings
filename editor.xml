<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="18" />
    <option name="FONT_REGULAR_SUB_FAMILY" value="Regular" />
    <option name="FONT_BOLD_SUB_FAMILY" value="Bold" />
    <option name="LINE_SPACING" value="1.8" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="disabledHintProviderIds">
      <set>
        <option value="JavaScript.js.type.hints" />
      </set>
    </option>
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
  </component>
  <component name="JavaCodeFoldingSettings">
    <option name="collapseConstructorGenericParameters" value="false" />
    <option name="collapseLambdas" value="false" />
    <option name="collapseOneLineMethods" value="false" />
  </component>
  <component name="LiveEditOptions" chromeAutoDelay="150" chromeUpdateJSOnChanges="true" nodeAutoDelay="150" nodeUpdateOnChanges="true" restartIfHotSwapFailed="true" updateFilesToCompile="true" />
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>