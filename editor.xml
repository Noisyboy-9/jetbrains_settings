<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_IMPORTS" value="false" />
    <option name="COLLAPSE_DOC_COMMENTS" value="true" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="20" />
    <option name="LINE_SPACING" value="1.5" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="PHP" value="true" />
      </map>
    </option>
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="SHOW_BREADCRUMBS" value="false" />
  </component>
  <component name="PhpCodeFoldingSettings">
    <option name="COLLAPSE_HERE_DOCS" value="true" />
    <option name="COLLAPSE_METHOD_BODY" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>